<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= data.page.title || "The Middleman" %></title>
    
    <%= stylesheet_link_tag "normalize", "all" %>
    <%= javascript_include_tag "http://cdnjs.cloudflare.com/ajax/libs/jquery/1.8.2/jquery.min.js" %>
		<%= javascript_include_tag "http://cdnjs.cloudflare.com/ajax/libs/mustache.js/0.7.0/mustache.min.js" %>
		<%= javascript_include_tag "mixer" %>
  </head>
  
  <body class="<%= page_classes %>">
    <%= yield %>
  </body>
</html>